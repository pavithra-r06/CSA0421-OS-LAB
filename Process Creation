#include <stdio.h>
#include <unistd.h>

int main() {
    printf("Before fork\n");

    pid_t pid = fork();

    if (pid < 0) {
        printf("Fork failed\n");
    } 
    else if (pid == 0) {
        printf("Child Process, PID = %d\n", getpid());
    } 
    else {
        printf("Parent Process, PID = %d\n", getpid());
    }

    return 0;
}
